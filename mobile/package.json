{
  "name": "regenerative-platform-mobile",
  "version": "1.0.0",
  "description": "Super App Regenerativo - Aplicativo Mobile Nativo",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace RegenerativePlatform.xcworkspace -scheme RegenerativePlatform -configuration Release -destination generic/platform=iOS -archivePath RegenerativePlatform.xcarchive archive",
    "bundle:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle",
    "bundle:ios": "react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/main.jsbundle",
    "postinstall": "cd ios && pod install"
  },
